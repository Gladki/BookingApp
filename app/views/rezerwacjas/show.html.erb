<%- model_class = Rezerwacja -%>
<div class="page-header">
  <h3><%=t '.title', :default => model_class.model_name.human %></h3>
</div>

<dl class="dl-horizontal">
  <dt><strong>Wycieczka:</strong></dt>
  <dd><%= FindWycieczkaByID(@rezerwacja.nazwa) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imie) %>:</strong></dt>
  <dd><%= @rezerwacja.imie %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nazwisko) %>:</strong></dt>
  <dd><%= @rezerwacja.nazwisko %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @rezerwacja.email %></dd>
  <dt><strong>Numer telefonu:</strong></dt>
  <dd><%= @rezerwacja.nrtelefonu %></dd>
  <dt><strong>Data urodzenia:</strong></dt>
  <dd><%= @rezerwacja.dataUrodzenia %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              rezerwacjas_index_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_rezerwacja_path(@rezerwacja), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              rezerwacja_path(@rezerwacja),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'JesteÅ› pewien?')) },
              :class => 'btn btn-danger' %>
</div>
